@BaseClass = class : "A player class"
[
	Name(string) : "Class Name" : "Class"
	Health(int) : "Maximum Health" : "125"
	MoveSpeed(float) : "Movement Speed" : "250"
	SpecialAbility(choices) : "Special Ability" : "0" =
	[
		0 : "None"
		1 : "Buildings"
	]
	Weapons(array:string) { editor = "AssetBrowse( weapon )" } : "Weapons"
	Hat(resource:vmdl) : "Hat" : ""
	RedShirt(resource:vmdl) : "Red Shirt" : ""
	BlueShirt(resource:vmdl) : "Blue Shirt" : ""
	RedPants(resource:vmdl) : "Red Pants" : ""
	BluePants(resource:vmdl) : "Blue Pants" : ""
	Shoes(resource:vmdl) : "Shoes" : ""
]