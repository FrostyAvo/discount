@BaseClass = weapon : "A weapon"
[
	Name(string) : "Weapon Name" : "Weapon"
	ViewModelPath(resource:vmdl) : "View Model"
	WorldModelPath(resource:vmdl) : "World Model"
	AttackType(choices) : "Attack Type" : "0" =
	[
		0 : "Bullet"
		1 : "Melee"
		2 : "Projectile"
	]
	PrimaryFireSound(string) : "Primary Fire Sound" : ""
	HoldType(choices) : "Hold Animation Type" : "0" =
	[
		0 : "None"
		1 : "Pistol"
		2 : "SMG"
		3 : "Shotgun"
		4 : "Item"
	]
	BulletsPerShot(int) : "Bullets Per Shot" : "1"
	PrimaryFireRate(float) : "Primary Fire Rate" : "1"
	AmmoPerShot(int) : "Ammo Per Shot" : "1"
	ClipSize(int) : "Clip Size" : "1"
	ReserveAmmo(int) : "Reserve Ammo" : "1"
	AmmoPerReload(int) : "Ammo Per Reload" : "1"
	ReloadTime(float) : "Reload Time" : "1"
	ReloadSound(string) : "Reload Sound" : ""
	Damage(float) : "Damage Per Bullet" : "5"
	Knockback(float) : "Knockback Per Bullet" : "100"
	SpreadAngle(float) : "Spread Cone Angle" : "0"
	Range(float) : "Range" : "2000"
	DeployTime(float) : "Deploy Time" : "0"
	CanHeadshot(bool) : "Can Headshot" : "false"
	WindupTime(float) : "Windup Time" : "0"
	WindupSound(string) : "Windup Sound" : ""
	Healing(bool) : "No Damage, Heals Teammates" : "false"
	GravityAffected(bool) : "Affected by Gravity (Projectiles)" : "false"
	Explosive(bool) : "Explosive (Projectiles)" : "false"
	ExplosionRadius(float) : "Explosion Radius (Projectiles)" : "100"
	DisarmOnFirstHit(bool) : "Don't Damage After First Hit (Projectiles)" : "false"
	Sticky(bool) : "Stick to Impact Point (Projectiles)" : "false"
	EmitSmoke(bool) : "Emit Smoke (Projectiles)" : "false"
	ProjectileSpeed(float) : "Projectile Speed" : "1000"
	ProjectileLifetime(float) : "Projectile Lifetime" : "10"
]